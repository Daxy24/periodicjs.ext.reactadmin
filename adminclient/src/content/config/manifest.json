    "/r-admin/content/accounts":{
      "layout":{
        "component":"Hero",
        "props":{
          "size":"isFullheight"
        },
        "children":[
          {
            "component":"HeroBody",
            "props":{

            },
            "asyncprops":{
              "healthcheck":[
                "healthcheckStatus"
              ]
            },
            "children":[{
              "component":"Container",
              "props":{
              },
              "children":[
                {
                  "component":"div",
                  "props":{
                    "dangerouslySetInnerHTML":{
                      "__html":"<h1>Accounts: the most used line in code</h1>"
                    }
                  }
                },
                {
                  "component": "ResponsiveCard",
                  "props": {
                    "cardTitle": "Application Results(# Records)",
                    "leftIcon":true
                  },
                  "children": [{
                    "component": "ResponsiveTable",
                    "props": {
                      "limit":10,
                      "filterSearch":true,
                      "tableSearch":true,
                      "flattenRowData":true,
                      "baseUrl":"/r-admin/contentdata/accounts?format=json",
                      "dataMap":[{
                        "key":"rows",
                        "value":"accounts"
                      },{
                        "key":"numItems",
                        "value":"accountscount"
                      },{
                        "key":"numPages",
                        "value":"accountpages"
                      }],
                      "headerLinkProps":{
                        "style":{
                          "textDecoration":"none"
                        }
                      },
                      "headers": [
                        { "label": "#", "value":"--idx-ctr--" },
                        { 
                          "label": "Id", 
                          "sortid":"_id", 
                          "link":{
                            "baseUrl":"/r-admin/contentdata/accounts/:id",
                            "params":[{"key":":id","val":"_id"}]
                          },
                          "linkProps":{
                            "style":{
                              "textDecoration":"none"
                            }
                          } 
                        },
                        { "label": "Email", "sortid":"email", "sortable":true },
                        { 
                          "label": "First Name", 
                          "sortid":"firstname", 
                          "sortable":true, 
                          "columnProps":{
                            "style":{
                              "backgroundColor":"lightgrey",
                              "color":"grey"
                            }
                          }, 
                          "headerColumnProps":{
                            "style":{
                              "backgroundColor":"black",
                              "color":"white"
                            }
                          }
                        }, 
                        { "label": "Last Name", "sortid":"lastname", "sortable":true }, 
                        { "label": "Create Date", "sortid":"createdat", "momentFormat":"lll", "sortable":true }
                      ]
                    },
                    "asyncprops":{
                      "rows":[
                        "tabledata","accounts"
                      ],
                      "numItems":[
                        "tabledata","accountscount"
                      ],
                      "numPages":[
                        "tabledata","accountpages"
                      ]
                    }
                  }]
                },
                {
                  "component":"ResponsiveForm",
                  "props":{
                    "notificationForm":true,
                    "flattenFormData":true,
                    "onSubmit":"func:this.props.debug",
                    "formgroups":[{
                      "formElements":[{
                        "type":"text",
                        "name":"0.email",
                        "label":"Email",
                        "value":"email"
                      },{
                        "type":"layout",
                        "value":{
                          "component":"div",
                          "children":[{
                            "component":"Title",
                            "size":"is2",
                            "children":"random text"
                          },{
                            "component":"span",
                            "children":"random text"
                          }]
                        }
                      },{
                        "type":"submit",
                        "value":"debug"
                      
                      }]
                    },{
                      "formElements":[{
                        "type":"code",
                        "name":"jsCode",
                        "passProps": {
                          "mode": "javascript"
                        },
                        "value":"console.log('this is a code mirror);"
                 
                      },{
                        "type":"select",
                        "name":"testSelect",
                        "label":"status",
                        "value":"off",
                        "options":[{
                          "label":"true",
                          "value":true
                        },{
                          "label":"false",
                          "value":false
                        },{
                          "label":"Turn Off",
                          "value":"off"
                        },{
                          "label":"Turn On",
                          "value":"on"
                        }]
                      }]
                    }]
                  },
                  "asyncprops":{
                    "formdata":["tabledata","accounts"]
                  }
                },
                {
                  "component": "Tabs",
                  "props":{
                    "style":{
                    }
                  },
                  "children": [{
                    "component": "TabGroup",
                    "props":{
                      "className":"breadcrumb",
                      "style":{
                        "border":"1px solid black",
                        "color":"red"
                      }
                    },
                    "children": [{
                      "component": "Tab",
                      "props": {
                        "isActive": true,
                        "style": {}
                      },
                      "children": [{
                        "component": "ResponsiveButton",
                        "props": {
                          "onClick": "func:this.props.reduxRouter.push",
                          "onclickProps": "/crm/customers",
                          "style": {}
                        },
                        "children": "customer"      
                      }]
                    },
                    {
                      "component": "Tab",
                      "props": {
                        "isActive": false,
                        "style": {}
                      },
                      "children": [{
                        "component": "ResponsiveButton",
                        "props": {
                          "onClick": "func:this.props.reduxRouter.push",
                          "onclickProps": "/crm/applications",
                          "onclickBaseUrl":"/r-admin/contentdata/accounts/:id",
                          "onclickLinkParams":[{"key":":id","val":"headerHost"}],
                          "style": {}
                        },
                        "asyncprops":{
                          "onclickPropObject":["tabledata","request"]
                        },
                        "children": "application"      
                      }]
                    }]
                  }]
                },
                {
                  "component":"ResponsiveCard",
                  "props":{
                    "cardTitle":"upload new asset",
                    "iconImage":{
                      "size":"is48X48"
                    },
                    "icon":"https://s3-us-west-2.amazonaws.com/promisefinancial.com/cloudfiles%2F2015%2F07%2F03%2F800x800-yaw-etse-portrait-min.jpg",
                    "iconDown":"https://s3-us-west-2.amazonaws.com/promisefinancial.com/cloudfiles%2F2015%2F07%2F03%2F800x800-yaw-etse-portrait-min.jpg",
                    "iconUp":"https://promisefinancial.com/favicon.png"
                  },
                  "children":[
                    {
                      "component":"ResponsiveForm",
                      "props":{
                        "onSubmit":{
                          "url":"http://localhost:8786/r-admin/contentdata/assets",
                          "options":{
                            "method":"post"
                          }
                        },
                        "card":{
                          "props":{
                            "cardTitle":"Upload New Asset"
                          }
                        },
                        "formgroups":[
                          {
                            "formElements":[
                              {
                                "type":"text",
                                "name":"title",
                                "label":"title"
                              },
                              {
                                "type":"select",
                                "name":"status",
                                "label":"status",
                                "options":[
                                {
                                  "label":"valid",
                                  "value":"VALID"
                                },
                                {
                                  "label":"invalid",
                                  "value":"INVALID"
                                }]
                              }
                            ]
                          },
                          {
                            "formElements":[
                              {
                                "type":"textarea",
                                "value":"no desc",
                                "name":"description",
                                "label":"description",
                                "passProps":{
                                  "isExpanded":true
                                }
                              }
                            ]
                          },
                          {
                            "formElements":[
                              {
                                "type":"submit",
                                "value":"upload"
                              }
                            ]
                          }
                        ]
                      }
                    }
                  ]
                }
              ]
            }]
          }
        ]
      },
      "resources":{
        "tabledata":"/r-admin/contentdata/accounts?format=json&limit=10"
      },
      "onFinish":"render",
      "pageData":{
        "title":"Accounts",
        "navLabel":"Accounts"
      }
    }
  }
}