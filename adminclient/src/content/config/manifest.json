    "/home":{
      "layout":{
        "component":"Hero",
        "props":{
          "size":"isFullheight"
        },
        "children":[
          {
            "component":"HeroBody",
            "props":{

            },
            "asyncprops":{
              "healthcheck":[
                "healthcheckStatus"
              ]
            },
            "children":[
              {
                "component":"Container",
                "props":{

                },
                "children":[
                  {
                    "component":"div",
                    "props":{
                      "dangerouslySetInnerHTML":{
                        "__html":"<h1>FooBar: the most used line in code</h1>"
